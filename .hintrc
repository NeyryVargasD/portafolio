{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select",
          "backdrop-filter",
          "scroll-behavior",
          "aspect-ratio"
        ]
      }
    ],
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/paint": "off"
  }
}